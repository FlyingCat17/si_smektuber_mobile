<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".screen.home.Notification"
    android:paddingLeft="22dp"
    android:paddingRight="22dp"
    >

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/successmark"
        android:layout_width="217dp"
        android:layout_height="182dp"
        android:layout_marginLeft="61dp"
        android:layout_marginRight="82dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="185dp"
        android:clipToOutline="true"
        android:background="@drawable/icon_no_internet"
        />

    <TextView
        android:id="@+id/gagal_masuk"
        android:layout_width="221dp"
        android:layout_height="31dp"
        android:layout_marginLeft="70dp"
        android:layout_marginRight="69dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="374dp"
        android:text="@string/tidak_ada_i"
        android:textAppearance="@style/gagal_masuk"
        android:gravity="center_horizontal|top"
        />

    <TextView
        android:layout_width="316dp"
        android:layout_height="46dp"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="22dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="413dp"
        android:text="@string/pastikan_in"
        android:textAppearance="@style/pastikan_ka"
        android:lineSpacingExtra="4sp"
        android:translationY="-2.25sp"
        android:gravity="center_horizontal|top"
        />

    <include
        android:id="@+id/BtnNotifNoInternet"
        layout="@layout/button_coba_lagi"
        android:layout_alignParentTop="true"
        android:layout_marginTop="500dp"
        android:layout_width="match_parent"
        android:layout_height="75dp"/>


</RelativeLayout>